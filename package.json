{
  "name": "Milaidy",
  "version": "2.0.0-alpha.7",
  "description": "Cute agents for the acceleration",
  "homepage": "https://github.com/milady-ai/milaidy",
  "author": {
    "name": "Milady AI",
    "email": "hi@milady.ai"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/milady-ai/milaidy.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "build/src/index.js",
  "bin": {
    "milaidy": "milaidy.mjs"
  },
  "exports": {
    ".": "./dist/index.js",
    "./cli-entry": "./milaidy.mjs",
    "./eliza": "./dist/runtime/eliza.js"
  },
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "build": "bun run tsdown && cp src/plugins/telegram-enhanced/package.json dist/plugins/telegram-enhanced/package.json && scripts/rt.sh scripts/write-build-info.ts && cd apps/app && ../../scripts/rt.sh run build",
    "check": "scripts/rt.sh run typecheck && scripts/rt.sh run lint",
    "format": "bun run @biomejs/biome format .",
    "lint": "bun run @biomejs/biome check .",
    "dev": "scripts/rt.sh scripts/dev-all.mjs",
    "milaidy": "scripts/rt.sh scripts/run-node.mjs",
    "postinstall": "node ./scripts/link-browser-server.mjs",
    "start": "cd apps/app && ../../scripts/rt.sh run electron",
    "test": "node test/scripts/test-parallel.mjs",
    "test:coverage": "bun run vitest run --coverage",
    "test:e2e": "bun run vitest run --config vitest.e2e.config.ts",
    "test:live": "MILAIDY_LIVE_TEST=1 bun run vitest run --config vitest.e2e.config.ts test/wallet-live.e2e.test.ts test/api-auth-live.e2e.test.ts test/cloud-providers.e2e.test.ts",
    "typecheck": "bun run tsc --noEmit"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "@elizaos/core": "next",
    "@elizaos/plugin-google-genai": "next",
    "@elizaos/plugin-knowledge": "next",
    "@elizaos/plugin-ollama": "next",
    "@elizaos/plugin-openai": "next",
    "@elizaos/plugin-openrouter": "next",
    "@elizaos/plugin-todo": "next",
    "@elizaos/tui": "next",
    "@mariozechner/pi-ai": "^0.52.9",
    "@noble/curves": "^2.0.1",
    "@capacitor-community/electron": "^5.0.1",
    "bonjour-service": "^1.3.0",
    "chalk": "^5.4.1",
    "chokidar": "~3.5.3",
    "commander": "^14.0.0",
    "dotenv": "^17.2.3",
    "drizzle-orm": "0.45.1",
    "electron-is-dev": "~2.0.0",
    "electron-serve": "~1.1.0",
    "electron-unhandled": "~4.0.1",
    "electron-updater": "^5.3.0",
    "electron-window-state": "^5.0.3",
    "ethers": "^6.16.0",
    "json5": "^2.2.3",
    "node-llama-cpp": "^3.15.1",
    "pg": "^8.16.3",
    "telegraf": "^4.16.3",
    "whisper-node": "^1.1.1",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
    "@types/node": "^25.2.0",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^4.0.18",
    "rimraf": "^5.0.0",
    "tsdown": "^0.20.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "overrides": {
    "drizzle-orm": "0.45.1",
    "@elizaos/core": "2.0.0-alpha.10"
  }
}
